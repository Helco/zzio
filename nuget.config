<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    <add key="local" value="nuget-feed" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget">
      <package pattern="*" />
      <package pattern="Veldrid.SPIRV" />
    </packageSource>
    <packageSource key="local">
      <package pattern="Veldrid"/>
      <package pattern="Veldrid*"/>
      <package pattern="DefaultEcs"/>
      <package pattern="ImGui.NET"/>
      <package pattern="ImGuizmo.NET"/>
      <package pattern="Mlang"/>
      <package pattern="Mlang*"/>
      <package pattern="Remotery.*"/>
      <package pattern="MojoAL*"/>
    </packageSource>
  </packageSourceMapping>
</configuration>
